{
  "name": "practical",
  "version": "1.0.0",
  "description": "practical challenge workspace root",
  "private": true,
  "scripts": {
    "dev": "pnpm build:watch",
    "dev:ui": "parcel serve application/ui/src/index.html",
    "test": "ava",
    "test:watch": "pnpm test -- --watch",
    "test:u": "pnpm test -- --update-snapshots",
    "build": "parcel build {application,lib}/*",
    "build:watch": "parcel watch {application,lib}/*",
    "start": "pnpm exec nodemon application/api/dist/api.js",
    "tscheck": "tsc -p . --noEmit"
  },
  "author": "Cody Averett",
  "license": "MIT",
  "ava": {
    "files": [
      "**/*.test.mjs"
    ]
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@parcel/config-default": "^2.4.1",
    "@parcel/core": "^2.4.1",
    "@parcel/packager-ts": "2.5.0",
    "@parcel/transformer-typescript-tsc": "^2.4.1",
    "@parcel/transformer-typescript-types": "2.5.0",
    "@parcel/transformer-vue": "^2.5.0",
    "@types/node": "^17.0.23",
    "ava": "^4.2.0",
    "nodemon": "^2.0.15",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
